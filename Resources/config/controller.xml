<?xml version="1.0" encoding="utf-8" ?>
<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">
    
    <services>

        <!-- Registry -->
        <service id="oneup_uploader.controller_registry" class="Oneup\UploaderBundle\Model\ControllerRegistry" public="false" />

        <!-- Controller definitions -->
        <service id="oneup_uploader.controller.dropzone" class="Oneup\UploaderBundle\Controller\DropzoneController" public="false">
            <tag name="oneup_uploader.controller" handles="dropzone" />
        </service>
    </services>

</container>